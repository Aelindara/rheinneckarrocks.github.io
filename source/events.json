---
use: ["events", "usergroups"]
permalink: events.json
---
{
  "success": 1,
  "result": [
    {% for event in data.events %}
    {% set ugname = '' %}
    {% for usergroup in data.usergroups %}
      {% if event.usergroup == usergroup.title %}
        {% set ugname = [ usergroup.name, ': ']|join %}
      {% endif %}
    {% endfor %}
    {
    "id": {{ loop.index0 }},
    "title": "{{ ugname }}{{ event.meta.title }}",
    "url": "{{ event.meta.link }}",
    "class": "{{ event.meta.type|type_css_class }}",
    "start": "{{ event.meta.date|unixtime }}",
    "end": "{{ event.meta.date|unixtime }}"
    }{% if not loop.last %},{% endif %}
    {% endfor %}
  ]
}
